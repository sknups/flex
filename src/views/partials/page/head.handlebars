<meta property="title" content="{{title}}" />
<meta property="og:title" content="{{data.skuname}}" />
<meta property="og:description" content="{{data.description}}" />
<meta property="og:image" content="src="{{host}}/img/card/v1/og/{{data.thumbprint}}.png"" />
<meta property="og:url" content="src="{{host}}/img/card/v1/default/{{data.thumbprint}}.png"" />
<meta property="og:type" content="image" />
<meta property="og:locale" content="en_GB" />
<meta name="theme-color" content="#d800ff">
<!-- Twitter Card Tags -->
<meta property="twitter:card" content="summary_large_image" />
<meta property="twitter:image" content="{{host}}/img/card/v1/og/{{data.thumbprint}}.png" />
<meta property="twitter:text:title" content="{{title}}" />
<meta name="twitter:site" content="@GgSknups">
<meta name="twitter:title" content="{{data.skuname}}">
<meta name="twitter:image" content="{{data.certImageUrl}}">