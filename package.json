{
  "name": "flex-ui",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "build": "tsc && npm run build-scss",
    "build-scss": "rm -rf ./public/assets/css && node-sass ./src/scss -o ./public/assets/css",
    "start": "node ./dist/app.js",
    "scss": " node-sass ./src/scss -o ./public/assets/css && node-sass --watch ./src/scss -o ./public/assets/css",
    "dev": "export DEBUG=* && nodemon src/app.ts",
    "dev2": "nodemon src/app.ts",
    "debug": "export DEBUG=* && npm run start",
    "test": "jest --runInBand"
  },
  "dependencies": {
